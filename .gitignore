__pycache__/
*.pyc
*.pyd
*.so
*.ipynb_checkpoints/ # If you use Jupyter notebooks

# C extensions
*.dll
*.lib
*.exp

# Distribution / packaging
.Python
build/
dist/
*.egg-info/
*.egg
*.whl

# PyInstaller / cx_Freeze / Nuitka / py2app builds
*.spec
build/
dist/

# Unit test / coverage reports
.coverage
.pytest_cache/
htmlcov/
.nox/

# Editor/IDE files
.idea/             # JetBrains IDEs (PyCharm, IntelliJ, etc.)
.vscode/           # VS Code
*.sublime-project
*.sublime-workspace
*.DS_Store         # macOS
Thumbs.db          # Windows
*.swp              # Vim swap files

# Logs and databases
*.log
*.sqlite3          # Common for SQLite databases
*.db

# Environment variables / secrets
# IMPORTANT: This prevents your API keys and sensitive configurations from being committed!
.env
.flaskenv

